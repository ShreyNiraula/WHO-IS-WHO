  <body>
    <div class="container">
      <div class="row">
        <div
          id="custom-form"
          style="
            width: 70%;
            padding: 2rem;
            margin: auto auto;
            background-color: #d1d1d1;
            border-radius: 8px;
          "
        >
          <form method="POST">
            <div class="form-row">
              <div class="form-group col-md-4">
                <label for="First_Name">password</label>
                <input
                  class="form-control"
                  type="password"
                  name="Password"
                  id="Password"
                  required
                />
              </div>
            </div>
             
            <button
              type="submit"
              class="btn btn-secondary btn-lg btn-block mt-3"
            >
              Submit
            </button>
          </form>
        </div>
      </div>
    </div>
    <!-- jQuery and JS bundle w/ Popper.js -->
